{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "~8.2.0",
        "filament/filament": "^2.0",
        "filament/forms": "^2.0",
        "filament/spatie-laravel-translatable-plugin": "^2.0",
        "filament/tables": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "jeffgreco13/filament-breezy": "^1.5",
        "laravel-frontend-presets/tall": "^6.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^2.12"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.13",
        "doctrine/dbal": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.10",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "code-qualit-test": [
            "@php ./vendor/bin/pint --config ./pint.json --test"
        ],
        "pint:fix": "php ./vendor/bin/pint --config ./pint.json",
        "pint:test": "php ./vendor/bin/pint --config ./pint.json --test"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "filament/packages/*"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
